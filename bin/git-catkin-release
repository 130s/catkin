cat <<EOF
Usage: git catkin-release [command] ..
 
  Release catkin projects.

commands:
  
  export

EOF
}

USAGE=$(gcr_help)

gcr_export()
{
    
}

CMD=$1
shift

if eval gcr_$CMD $* ;then
  exit 0
else
